package com.atmospherebbdo.load.preloader {	/**	 * @author markhawley	 * 	 * PreloaderApplications expect these flashVars to be passed to them via	 * the containing HTML page. Set default values here to be used if they are	 * not present (like in the IDE.)	 */	public class PreloaderFlashVarsDefaults 	{		/**		 * URL of the primary application swf.		 */		public var mainSWF:String;				/**		 * pipe-delimited list of URLs that the preloader must load		 */		public var assets:String;				/**		 * Constructor.		 * 		 * @param	mainSWF	String, the URL of the primary application SWF.		 * @param	assets	String, a pipe-delimited list of URLs.		 */		public function PreloaderFlashVarsDefaults( mainSWF:String=null, assets:String=null )		{			this.mainSWF = mainSWF;			this.assets = assets;		}	}}