package com.atmospherebbdo.log {	import com.atmospherebbdo.assertions.fail;			/**	 * @author markhawley	 * 	 * Collection of static references to various log message formats.	 */	internal class LogMessageFormat 	{		private static var initialized:Boolean = false;				public static const DEFAULT:LogMessageFormat = new LogMessageFormat("%(level)s %(time)s: %(message)s (%(origin)s)");		public static const SHORT:LogMessageFormat = new LogMessageFormat("%(level)s: %(message)s (%(origin)s)");		public static const ORIGIN_FIRST:LogMessageFormat = new LogMessageFormat("%(origin)s: %(message)s (%(level)s)");				private var format:String;				{			initialized = true;		}				public function LogMessageFormat(s:String)		{			if (initialized)			{				fail("This class is intended to only be instantiated at " +					"compile time.");			}			format = s;		}				public function toString() :String		{			return format;		}	}}