package com.atmospherebbdo.util {	/**	 * @author markhawley	 * 	 * An interface for things that can be compared (but that the usual	 * operators will become highly confused by)	 */	public interface IComparable 	{		function equals( obj:IComparable ) :Boolean;		function lessThan( obj:IComparable ) :Boolean;		function greaterThan( obj:IComparable ) :Boolean;		function valueOf() :Object;	}}