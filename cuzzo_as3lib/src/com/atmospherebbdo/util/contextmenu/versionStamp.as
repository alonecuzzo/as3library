package com.atmospherebbdo.util.contextmenu {	import com.atmospherebbdo.util.getBuildVersion;		import flash.display.InteractiveObject;	import flash.ui.ContextMenuItem;			/**	 * @author markhawley	 * 	 * Adds a version stamp to an interactive object.	 * 	 * @param	io	InteractiveObject, an object to be stamped	 * @param	version	String, the version to stamp	 */	public function versionStamp( io:InteractiveObject, 		version:String=null ) :void	{		if (null == version)		{			version = getBuildVersion();		}		var cmi:ContextMenuItem = new ContextMenuItem( "Version: " + version );		customizeContextMenu(io, cmi);	}}