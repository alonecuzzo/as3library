package com.atmospherebbdo.util.displayobject{	import com.atmospherebbdo.errors.AbstractMethodCallError;	import com.atmospherebbdo.util.instantiatedAs;			/**	 * Alignment enum used for display object utilities.	 * 	 * @author hawleym	 */		public class AbstractAlignment	{		protected var name:String;				/**		 * Private constructor, as Alignments are only constructed within this class.		 * 		 * @param	name	String, the primitive value of an Alignment		 */		public function AbstractAlignment( name:String ) 		{			if (instantiatedAs(this, AbstractAlignment))			{				throw new AbstractMethodCallError("Cannot " +					"instantiate Abstract class.");			}			this.name = name;		}				/**		 * Return name for string value.		 * 		 * @return String		 */		public function toString() :String		{			return name;		}				/**		 * Return value for primitive usage.		 * 		 * @return String		 */		public function valueOf() :String		{			return name;		}	}}