package com.atmospherebbdo.collections {	/**	 * @author markhawley	 * 	 * A Collection that counts the number of times an object 	 * appears in the collection. Suppose you have a Bag that 	 * contains {a, a, b, c}. Calling getCount(Object) on a would 	 * return 2, while calling uniqueSet() would return {a, b, c}. 	 */	public interface IBag extends ICollection	{		/**		 * returns the number of obj found in the bag.		 */		function getCount( obj:* ) :int;				/**		 * returns an ISet containing one item of each found in the IBag.		 */		function uniqueSet() :ISet;				/**		 * returns an IBag, the set difference of this and the argument		 */		function difference( other:IBag ) :IBag;				/**		 * returns an IBag, the set union of this and the argument		 */		function union( other:IBag ) :IBag;				/**		 * returns an IBag, the set intersection of this and the argument		 */		function intersection( other:IBag ) :IBag;	}}