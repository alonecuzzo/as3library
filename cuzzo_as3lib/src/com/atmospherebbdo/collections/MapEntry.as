package com.atmospherebbdo.collections {	/**	 * @author markhawley	 * 	 * An entry in a Map collection. 	 */	public class MapEntry 	{		private var _value:*;		private var _key:*;				/**		 * Constructor.		 * 		 * @param	key	Object		 * @param	value	Object		 */		public function MapEntry( key:*, value:* ) 		{			_value = value;			_key = key;		}				/**		 * Returns the entry's key.		 * 		 * @return *		 */		public function getKey() :*		{			return _key;		}				/**		 * Returns the entry's value.		 * 		 * @return *		 */		public function getValue() :*		{			return _value;		}				/**		 * Sets the entry's value.		 * 		 * @param v	* new value		 * 		 * @return * old value		 */		public function setValue( v:* ) :*		{			var old:* = _value;			_value = v;			return old;		}	}}