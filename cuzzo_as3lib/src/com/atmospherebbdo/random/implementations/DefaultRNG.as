package com.atmospherebbdo.random.implementations {	/**	 * @author markhawley	 */	public class DefaultRNG implements IRNGImplementation 	{		public function DefaultRNG()		{					}				public function setSeed(s:uint):void		{			// ingores the seed, unfortunately						if (s != 0)			{				trace("WARNING: Default IRNGImplementation must ignore seeds.");			}		}				public function getSeed() :uint		{			// no seed						trace("WARNING: Default IRNGImplementation may not be seeded.");						return 0;		}				public function random():Number		{			return Math.random();		}	}}