package com.atmospherebbdo.load {	import flash.events.IEventDispatcher;	
	
	import com.atmospherebbdo.collections.IMap;		import flash.display.AVM1Movie;	import flash.display.Bitmap;	import flash.display.BitmapData;	import flash.display.MovieClip;	import flash.media.Sound;	import flash.net.NetStream;	import flash.utils.ByteArray;		/**	 * @author markhawley	 * 	 * An interface for things that load other files. (Like MultiLoader.)	 */	public interface ILoader extends IEventDispatcher	{		function add( loadable:LoadItemData ) :void;		function getLoadData( loadable:LoadItemData ) :ILoadData;		function getContent( loadable:LoadItemData ) :*;		function getAVM1Movie( loadable:LoadItemData ):AVM1Movie;		function getBinary( loadable:LoadItemData ):ByteArray;		function getXML( loadable:LoadItemData ) :XML;		function getText( loadable:LoadItemData ) :String;		function getNetStream( loadable:LoadItemData ):NetStream;		function getBitmap( loadable:LoadItemData ) :Bitmap;		function getBitmapData( loadable:LoadItemData ) :BitmapData;		function getSound( loadable:LoadItemData ) :Sound;		function getMovieClip( loadable:LoadItemData ) :MovieClip;		function getNetStreamMetaData( loadable:LoadItemData ):Object;		function start() :void;		function pause() :void;		function resume() :void;				function get bytesLoaded() :int;		function get bytesTotal() :int;		function get contents() :IMap;	}}